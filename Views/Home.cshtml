@using Umbraco.Cms.Web.Common.PublishedModels;
@inherits Umbraco.Cms.Web.Common.Views.UmbracoViewPage<ContentModels.Home>
@using ContentModels = Umbraco.Cms.Web.Common.PublishedModels;
@{
	Layout = "Master.cshtml";
}

<h1>@Model.PageHeading</h1>

<form>
	<label for="csvFileUpload">Upload csv file</label>
	<input type="file"
		id="csvFileUpload" name="csvFileUpload"
		accept="csv">
	<button id="csvBtn">Upload</button>
</form>

<script>
	const addCSV = (ev) => {
		ev.preventDefault();
		let csvFile = document.querySelector('#csvFileUpload').value; 
		
	}
	
	document.addEventListener('DOMContentLoaded', ()=> {
		document.querySelector('#csvBtn').addEventListener('click', addCSV);
	})
</script>